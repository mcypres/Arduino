#include SensorHMC5883Multi.h

SensorHMC5883Multi::SensorHMC5883Multi(HardwareSerial *ptOutputSer,
					Logger *myLogger,
					float declinationAngleDegres,
					float declinationAnglemin,
					float xOffset,
					float yOffset,
					float zOffset,
					float speedCoef,
					int id
					int port):SensorHMC5883(ptOutputSer,
							myLogger,
							declinationAngleDegres,
							declinationAnglemin,
							xOffset,
							yOffset,
							zOffset,
							speedCoef,
							id){
 
 	this.port=port;
 	}